   -- Char_Height --
   -----------------

   function Char_Height (Font : BMP_Font) return Natural is
   begin
      case Font is
         when Font8x8   => return 8;
         when Font12x12 => return 12;
         when Font16x24 => return 24;
      end case;
   end Char_Height;

