with "../../boards/stm32f429_discovery/stm32f429_discovery.gpr";

project Sudoku_STM32F429Disco extends "../shared/common/common.gpr" is

   for Runtime ("Ada") use STM32F429_Discovery'Runtime("Ada");
   for Object_Dir use "../shared/sudoku/obj/stm32f429disco";
   for Create_Missing_Dirs use "True";

   for Main use ("sudoku_gui.adb");
   for Source_Dirs use ("../shared/sudoku/src");

   package Linker is
      for Default_Switches ("ada") use
        ("-gc-section", "-Wl,--print-memory-usage");
   end Linker;

end Sudoku_STM32F429Disco;
